RefreshText10:

  If not (ExhDESClose or ExhDESOpen) then ExhDESPos = "%"
  If not (MinOaDESClose or MinOaDESOpen) then MinOaDESPos = "%"
  If not (OaD1ESClose or OaD1ESOpen) then OaD1ESPos = "%"

RefreshText11:

  If not (OaD2ESClose or OaD2ESOpen) then OaD2ESPos = "%"
  If not (RetDESClose or RetDESOpen) then RetDESPos = "%"
  If not (RetSD1ESClose or RetSD1ESOpen) then RetSD1ESPos = "%"

RefreshText12:

  If not (RetSD2ESClose or RetSD2ESOpen) then RetSD2ESPos = "%"
  If not (SupSD1ESClose or SupSD1ESOpen) then SupSD1ESPos = "%"
  If not (SupSD2ESClose or SupSD2ESOpen) then SupSD2ESPos = "%"

  Goto OnlyLine

